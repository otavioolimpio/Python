import tkinter as tk

def clicando():
    label.config(text="Ol√° Mundo")

janela = tk.Tk()
janela.geometry("400x300")
janela.title("Minha interface")

label = tk.Label(janela, text="Bem vindo")
label.pack()

botao = tk.Button(janela, text = "Clique aqui", command=clicando)
botao.pack()

janela.mainloop()
